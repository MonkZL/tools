<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SuperRadioButton">
        <attr name="first_title" format="string" />
        <attr name="second_title" format="string" />
        <attr name="first_title_color" format="color|integer" />
        <attr name="second_title_color" format="color|integer" />
        <attr name="first_title_checked_color" format="color|integer" />
        <attr name="second_title_checked_color" format="color|integer" />
        <attr name="first_title_size" format="dimension" />
        <attr name="second_title_size" format="dimension" />
        <attr name="offset" format="dimension" />
        <attr name="background_unchecked" format="color|integer" />
        <attr name="background_checked" format="color|integer" />
        <attr name="radius" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color|integer" />
    </declare-styleable>
</resources>
